RewriteBase /
RewriteEngine On
RewriteCond %{REQUEST_FILENAME}\.html -f
RewriteRule ^([^\.]+)$ $1.html [L]
RewriteCond %{REQUEST_FILENAME}\.php -f
RewriteRule ^([^\.]+)$ $1.php [L]
IndexIgnore *
Options -Indexes
ErrorDocument 404 http://salio.me/error.html
ErrorDocument 403 http://salio.me/error.html
